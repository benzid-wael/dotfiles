[user]
    username = [[GIT_NAME]]
    email = [[GIT_EMAIL]]
    name = [[GIT_NAME]]
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
	glog = log --graph
    clone-branches = "! git branch -a | sed -n \"/\\/HEAD /d; /\\/master$/d; /remotes/p;\" | xargs -L1 git checkout -t"
[color]
    ui = true
